{
  "expo": {
    "name": "RNcarousel",
    "slug": "RNcarousel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/compose-music.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/fashion-blogging.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/social-thinking.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/compose-music.png"
    }
  }
}
